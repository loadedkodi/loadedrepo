<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.loaded" name="Loaded Wizard" version="1.9.2" provider-name="Jack Finney">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">The Loaded Wizard and Builds!</summary>
    <description lang="en">The Loaded Wizard allows you to get access to the latest Loaded Builds!</description>
    <platform>all</platform>
  </extension>
</addon>